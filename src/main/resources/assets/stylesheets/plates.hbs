@page {
  size: A4 landscape;
  margin: 20px 24px 0 24px;
  padding: 0;
}

@page :first {
    size: A4 landscape;
    margin: 20px 12px 0 12px;
    padding: 0;
}

html,
body {
    line-height: 1;
    margin: 0;
    -fs-page-sequence: start;
    page-break-inside: avoid;
}

*,
:after,
:before {
    box-sizing: border-box;
    line-height: 1;
}

p {
    margin: 0;
    padding: 0;
}

p,
div,
span {
    font-family: "Calibri", sans-serif;
}

.separate-page {
    page-break-after: always;
}

.separate-page > table {
    width: 100%;
}

.table-half {
    width: 49%;
}

.gap {
    width: 2%;
}

.vtg6.notes,
.vtg7.notes {
    padding-right: 20px;
}

.vtg6.notes {
    font-size: 15px;
}

.vtg7.notes {
    font-size: 10px;
}

.vtg6.notes > p,
.vtg7.notes > p,
.vtg7.notes > ol,
.vtg7.notes > ol {
    margin-top: .25em;
    margin-bottom: 0.25em;
}

.vtg6.notes ol,
.vtg7.notes ol {
    padding-left: 10px;
}

.vtg6.notes li,
.vtg7.notes li {
    padding-left: 10px;
    padding-bottom: 5px;
}

.vtg7 {
    border-collapse: collapse;
    border: none;
}

.vtg6 {
    border-collapse: collapse;
    border: none;
}

